<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sample.library.useandroidhttpasync" >

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-sdk android:minSdkVersion="15" android:targetSdkVersion="18"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light"
        >
        <activity
            android:name="com.example.sample.library.useandroidhttpasync.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.sample.library.useandroidhttpasync.activities.HttpUrlConnectionActivity" >
        </activity>
        <activity
            android:name="com.example.sample.library.useandroidhttpasync.activities.AndroidHttpAsyncActivity" >
        </activity>
    </application>

</manifest>
